{
  "name": "immediate",
  "version": "3.3.0",
  "description": "A cross browser microtask library",
  "contributors": [
    "Domenic Denicola <domenic@domenicdenicola.com> (http://domenicdenicola.com)",
    "Donavon West <github@donavon.com> (http://donavon.com)",
    "Yaffle",
    "Calvin Metcalf <calvin.metcalf@gmail.com>"
  ],
  "testling": {
    "files": "test/tests.js",
    "browsers": [
      "ie/6..latest",
      "chrome/latest",
      "firefox/3..latest",
      "opera/10..latest",
      "safari/4..latest",
      "iphone/latest",
      "ipad/latest",
      "android-browser/latest"
    ]
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/calvinmetcalf/immediate.git"
  },
  "browser": "dist/immediate.browser.js",
  "bugs": "https://github.com/calvinmetcalf/immediate/issues",
  "main": "dist/immediate.node.js",
  "scripts": {
    "build": "npm run build-js && npm run minify",
    "minify": "IMMEDIATE_ENV=umd rollup -c rollup.min.js --no-freeze --format umd --name immediate > dist/immediate.min.js",
    "build-js": "npm run build-umd && npm run build-browser && npm run build-node",
    "pretest": "npm run build-node",
    "test": "jshint lib/*.js && node test/tests.js | tspec",
    "build-umd": "IMMEDIATE_ENV=umd rollup -c --no-freeze --format umd --name immediate > dist/immediate.js",
    "build-browser": "IMMEDIATE_ENV=browser rollup -c --no-freeze   --format cjs > dist/immediate.browser.js",
    "build-node": "IMMEDIATE_ENV=node rollup -c --no-freeze   --format cjs > dist/immediate.node.js"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@rollup/plugin-babel": "^5.0.3",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "jshint": "^2.5.1",
    "rollup": "^2.15.0",
    "rollup-plugin-terser": "^7.0.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.0.0"
  }
}
